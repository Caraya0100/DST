                                        <lvc:CartesianChart.AxisX>
                                            <lvc:Axis Title="Mes" Labels="{Binding LabelsAnioFiscal}"></lvc:Axis>
                                        </lvc:CartesianChart.AxisX>
                                        <lvc:CartesianChart.AxisY>
                                            <lvc:Axis Title="Reubiciones" LabelFormatter="{Binding FormatoReubicaciones}"></lvc:Axis>
                                        </lvc:CartesianChart.AxisY>
                                    </lvc:CartesianChart>
