                                    <Label x:Name="lblDNumeroNoCapacitados" Content="N° trabajadores no capacitados: " Width="179" Height="26" HorizontalAlignment="Left" HorizontalContentAlignment="left" VerticalContentAlignment="Center" Margin="20,0,0,0"/>
                                    <TextBox x:Name="txtNumeroNoCapacitados" Width="50" Margin="15,0,0,0" HorizontalAlignment="Right" IsReadOnly="True"/>
                                    <lvc:Gauge x:Name="graficoDCapacitados" Uses360Mode="True" From="0" To="2" FromColor="Lime"
                            Value="{Binding ValorGraficoCapacitados}" LabelFormatter= "{Binding FormatoPorcentaje}" Margin="0,20,0,0" Width="778" Height="130"/>
                                    <Label x:Name="lblDCapacitados" Content="Porcentaje de trabajadores capacitados" Width="778" Height="28" HorizontalAlignment="Left" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"/>
