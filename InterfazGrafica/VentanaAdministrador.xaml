                                    <TextBox x:Name="txtNumeroReubicaciones" Width="50" Margin="15,0,0,0" HorizontalAlignment="Right" IsReadOnly="True"/>
                                    <Label x:Name="lblDNumeroCapacitados" Content="N° trabajadores capacitados: " Width="167" Height="26" HorizontalAlignment="Left" HorizontalContentAlignment="left" VerticalContentAlignment="Center" Margin="20,0,0,0"/>
