                                    <Label x:Name="lblDMensajeActualPlan" Content="No cumple con superar las ventas plan" Width="739" Height="40" HorizontalAlignment="Left" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Margin="20,0,0,30"/>
