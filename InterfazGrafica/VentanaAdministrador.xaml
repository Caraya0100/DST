                                        <lvc:CartesianChart.AxisX>
                                            <lvc:Axis Title="Mes" Labels="{Binding LabelsAnioFiscal}"></lvc:Axis>
                                        </lvc:CartesianChart.AxisX>
                                        <lvc:CartesianChart.AxisY>
                                            <lvc:Axis Title="Reubiciones" LabelFormatter="{Binding FormatoReubicaciones}"></lvc:Axis>
                                        </lvc:CartesianChart.AxisY>
                                    </lvc:CartesianChart>
                                </WrapPanel>
                                <WrapPanel x:Name="panelDatosMensuales" Height="600" Visibility="Collapsed">
                                    <lvc:CartesianChart x:Name="graficoVentasMes" Series="{Binding SeriesVentasMes}" LegendLocation="Right" Width="779" Height="178">
                                        <lvc:CartesianChart.AxisX>
                                            <lvc:Axis Title="Ventas" Labels="{Binding LabelsVentasMes}"></lvc:Axis>
                                        </lvc:CartesianChart.AxisX>
                                        <lvc:CartesianChart.AxisY>
                                            <lvc:Axis Title="Dinero" LabelFormatter="{Binding YFormatoVentasAnuales}"></lvc:Axis>
                                        </lvc:CartesianChart.AxisY>
                                    </lvc:CartesianChart>
